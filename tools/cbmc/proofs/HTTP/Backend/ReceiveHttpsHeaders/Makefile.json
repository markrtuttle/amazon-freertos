{
  "ENTRY": "ReceiveHttpsHeaders",

  # A CBMC pointer is an object id followed by an offset into the object.
  # The size of the offset is limited by the size of the object id.
  "CBMC_OBJECTID_BITS": "7",
  "CBMC_MAX_OBJECT_SIZE": "\"(UINT32_MAX>>(CBMC_OBJECTID_BITS+1))\"",
  "CBMC_MAX_BUFFER_SIZE": "20",
  "CBMC_BUFFER_UNWIND": "__eval {CBMC_MAX_BUFFER_SIZE} + 1",

  "CBMCFLAGS":
  [
    "--unwind 1",
    "--unwindset  _receiveHttpsMessage.0:{CBMC_BUFFER_UNWIND},_incrementNextLocationToWriteBeyondParsed.0:{CBMC_BUFFER_UNWIND}",


    # uninitialized global variables have arbitrary values
    "--nondet-static",

    # interesting checks in addition to memory safety
    "--div-by-zero-check",
    "--float-overflow-check",
    "--nan-check",
    "--pointer-overflow-check",
    "--undefined-shift-check",
    "--signed-overflow-check",
    "--unsigned-overflow-check"
  ],
  "OBJS":
  [
    "$(ENTRY)_harness.goto",
    "../../global_state_HTTP.goto",
    "$(FREERTOS)/libraries/c_sdk/standard/https/src/iot_https_client.goto"
  ],
  "INSTFLAGS":
  [
    # verified independently
    "--remove-function-body IotHttpsClient_Disconnect",
    "--remove-function-body _receiveHttpsBodyAsync",
    "--remove-function-body _receiveHttpsBodySync",

    # unused functions influencing function pointer removal
    "--remove-function-body _networkReceiveCallback",
    "--remove-function-body IotHttpsClient_Init",
    "--remove-function-body is_stubbed_IotClientCallbacks",
    "--remove-function-body is_stubbed_NetworkInterface"
  ],
  "INC":
  [
    "$(FREERTOS)/libraries/c_sdk/standard/https/include",
    "$(FREERTOS)/libraries/c_sdk/standard/https/src/private",
    "$(FREERTOS)/libraries/3rdparty/http_parser",
    "$(FREERTOS)/libraries/c_sdk/standard/common/include/",
    "$(FREERTOS)/libraries/abstractions/platform/include/",
    "$(FREERTOS)/libraries/abstractions/platform/freertos/include/",
    "../../../../include",
    "../.."
  ],
  "DEF":
  [
    "CBMC_OBJECTID_BITS={CBMC_OBJECTID_BITS}",
    "CBMC_MAX_OBJECT_SIZE={CBMC_MAX_OBJECT_SIZE}",
    "CBMC_MAX_BUFFER_SIZE={CBMC_MAX_BUFFER_SIZE}"
  ]
}
